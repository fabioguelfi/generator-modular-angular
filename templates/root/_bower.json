  "name": "<%= slugAppName %>",
  "version": "0.0.0",
  "authors": [
  ],
  "license": "none",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "json3": "latest",
    "es5-shim": "latest",
    "animate.css-scss": "latest",
